{
  "name": "subapp-decoration",
  "version": "0.1.0",
  "description": "icestark 基于 Vue 的子应用模板",
  "scripts": {
    "start": "vue-cli-service serve",
    "start:b": "vue-cli-service serve --mode b.dev",
    "start:c": "vue-cli-service serve --mode c.dev",
    "start:tmall:b": "vue-cli-service serve --mode tmall.b.dev",
    "start:tmall:c": "vue-cli-service serve --mode tmall.c.dev",
    "build:b:test": "vue-cli-service build --mode b.test",
    "build:b:uat": "vue-cli-service build --mode b.uat",
    "build:b:prod": "vue-cli-service build --mode b.prod",
    "build:c:test": "vue-cli-service build --mode c.test",
    "build:c:uat": "vue-cli-service build --mode c.uat",
    "build:c:prod": "vue-cli-service build --mode c.prod",
    "build:tmall:b:uat": "vue-cli-service build --mode tmall.b.uat",
    "build:tmall:b:prod": "vue-cli-service build --mode tmall.b.prod",
    "build:tmall:c:uat": "vue-cli-service build --mode tmall.c.uat",
    "build:tmall:c:prod": "vue-cli-service build --mode tmall.c.prod",
    "lint": "vue-cli-service lint --fix"
  },
  "dependencies": {
    "@ice/stark-app": "^1.0.1",
    "@ice/stark-data": "^0.1.2",
    "@vue/composition-api": "^1.0.3",
    "axios": "^0.21.0",
    "dayjs": "^1.9.7",
    "jsencrypt": "^3.2.1",
    "lz-crm": "^0.0.4",
    "lz-crm-vue-template": "^0.0.7",
    "lz-element-ui": "^2.15.8",
    "lz-event-tracking": "^1.0.16",
    "moment": "^2.29.1",
    "qs": "^6.9.4",
    "react": "^17.0.1",
    "sass": "^1.45.1",
    "vant": "^2.11.2",
    "vue": "^2.5.13",
    "vue-router": "^3.0.1",
    "vuedraggable": "^2.24.3",
    "vuex": "^3.6.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-decorators": "^7.0.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
    "@babel/plugin-proposal-function-sent": "^7.0.0",
    "@babel/plugin-proposal-json-strings": "^7.0.0",
    "@babel/plugin-proposal-numeric-separator": "^7.0.0",
    "@babel/plugin-proposal-throw-expressions": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-syntax-import-meta": "^7.0.0",
    "@vue/cli-plugin-babel": "^3.0.0",
    "@vue/cli-plugin-eslint": "^3.0.0",
    "@vue/cli-service": "^3.0.0",
    "@vue/eslint-config-airbnb": "^3.0.0",
    "babel-plugin-component": "^1.1.1",
    "babel-plugin-import": "^1.13.3",
    "babel-preset-es2015": "^6.24.1",
    "eslint-plugin-vue": "^4.5.0",
    "lint-staged": "^11.0.0",
    "sass-loader": "^7.1.0",
    "vue-template-compiler": "^2.5.13"
  },
  "repository": {
    "type": "git",
    "url": "git@49.4.78.8:customer_marketing/frontend/subapps/decoration.git"
  },
  "license": "MIT",
  "keywords": [
    "ice",
    "ice-scaffold"
  ],
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "iceworks": {
    "type": "vue",
    "adapter": "adapter-vue-v2"
  },
  "ideMode": {
    "name": "ice-vue"
  },
  "private": true,
  "originTemplate": "@vue-materials/icestark-child-app",
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,vue}": [
      "vue-cli-service lint --fix",
      "git add"
    ]
  }
}
